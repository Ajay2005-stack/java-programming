import java.util.Scanner;

public class RichestCustomerWealth {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

         Read dimensions m (rows) and n (columns)
        int m = sc.nextInt();
        int n = sc.nextInt();

        int[][] accounts = new int[m][n];

         Read the accounts grid
        for (int i = 0; i < m; i++) {
            for (int j = 0; j < n; j++) {
                accounts[i][j] = sc.nextInt();
            }
        }

        int maxWealth = 0;

         Calculate max wealth by summing each customer's accounts
        for (int i = 0; i < m; i++) {
            int wealth = 0;
            for (int j = 0; j < n; j++) {
                wealth += accounts[i][j];
            }
            if (wealth > maxWealth) {
                maxWealth = wealth;
            }
        }

        System.out.println("Richest Customer Wealth is " + maxWealth);
        sc.close();
    }
}
